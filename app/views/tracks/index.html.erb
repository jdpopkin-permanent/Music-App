<h2><%= "Tracks on #{@album.name}"%></h2>

<ul>
  <% @tracks.each do |track| %>
    <li>
      <%#= track.name %>
      <%= link_to "#{track.name}", band_album_track_url(@band, @album, track) %>
    </li>
  <% end %>
</ul>

<%= link_to "Back to #{@album.name}'s page", band_album_url(@band, @album) %>
<%= link_to "Back to #{@band.name}'s page", band_url(@band) %>